{
  "rewrites": [
    { "source": "/((?!assets|favicon|og-image|recursos|robots|sitemap|vite|widget).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
